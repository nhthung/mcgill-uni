public class TicTacToe{
  public static String[][] startGame(){
    String[][] gameBoard = new String[3][3];
    int index = 0
    for(int i = 0; i < 3; i++){
      for(int j = 0; j < 3; j++)
        gameBoard[i][j] = index++ + ": ";
    }
    return gameBoard;
  }
  
  public static void main(String[] args){
    for(int i = 0; i < 3; i++){
      for(int j = 0; j < 3; j++)
        System.out.print(startGame()[i][j] + "\t");
      System.out.println();
    }
  }
}
    